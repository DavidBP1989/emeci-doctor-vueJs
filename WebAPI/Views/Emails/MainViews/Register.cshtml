@model WebAPI.Models.DTO._Doctor.Register
<div style="margin-right: -15px; margin-left: -15px; display: table; content: ' '; width:100%;">
    <div style="float: left; position: relative; min-height: 1px; padding-right: 15px; padding-left: 15px;width:100%;">


        <div style="min-height:1px;position:relative;width:100%;" width="100%">
            <div style="-ms-flex-direction:column; -webkit-box-direction:normal; -webkit-box-orient:vertical; background-clip:border-box; background-color:#fff; border:none; border-radius:0.25rem; display:flex; flex-direction:column; min-width:0; position:relative; word-wrap:break-word" bgcolor="#ffffff">
                <div style="-ms-flex:1 1 auto; -webkit-box-flex:1; flex:1 1 auto; padding:1.25rem">
                    <form>
                        <h4 class="card-title" style="margin-bottom:1.5rem; margin-top:0; color:inherit; font-family:inherit; font-weight:500; line-height:1.2; font-size:1.2rem">Información del médico</h4>
                        <div style="-ms-flex-wrap:wrap;display:flex;margin-left:-5px;margin-right:-5px;">
                            <div class="form-group col-md-6 col-lg-3" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative;width:100%;margin-bottom:1rem;" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Nombre(s)</label>
                                <p>@Model.Name</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-3" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative;width:100%;margin-bottom:1rem;" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Apellido(s)</label>
                                <p>@Model.LastName</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-3" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative;width:100%;margin-bottom:1rem;" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">RFC</label>
                                <p>@Model.RFC</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-3" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative;width:100%;margin-bottom:1rem;" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">CURP</label>
                                <p>@Model.CURP</p>
                            </div>
                        </div>
                        <div class="form-row" style="-ms-flex-wrap:wrap;display:flex;margin-left:-5px;margin-right:-5px;">
                            <div class="form-group col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">No. Cédula profesional SEP</label>
                                <p>@Model.NoSEP_ProfessionalCertificate</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">No. S.S.A.</label>
                                <p>@Model.NoSSA</p>
                            </div>
                            <div class="form-group col-md-12 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">CMCP</label>
                                <p>@Model.NoCertification_CMCP</p>
                            </div>
                        </div>
                        <div class="form-row" style="-ms-flex-wrap:wrap; display:flex;margin-left:-5px; margin-right:-5px">
                            <div class="form-group col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Hospital de residencia</label>
                                <p>@Model.ProfessionalResidenceHospital</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Universidad que avala</label>
                                <p>@Model.UniversitySpecialty</p>
                            </div>
                            <div class="form-group col-md-12 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Cédula de especialidad SEP</label>
                                <p>@Model.SpecialtyCertificate</p>
                            </div>
                        </div>
                        <div class="form-row" style="-ms-flex-wrap:wrap; display:flex;margin-left:-5px; margin-right:-5px">
                            <div class="form-group col-md-6" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Nombre del colegio estatal</label>
                                <p>@Model.NameStateSchool</p>
                            </div>
                            <div class="form-group col-md-6" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Nombre de agrupación estatal</label>
                                <p>@Model.NameStateGrouping</p>
                            </div>
                        </div>
                        <h5 class="mt-3" style="margin-bottom:1.5rem; margin-top:1rem; color:inherit; font-family:inherit; font-weight:500; line-height:1.2; font-size:1.2rem">Domicilio particular</h5>
                        <div class="form-row" style="-ms-flex-wrap:wrap; display:flex; margin-left:-5px; margin-right:-5px">
                            <div class="form-group has-search col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Estado</label>
                                <p>@Model.State</p>
                            </div>
                            <div class="form-group has-search col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Ciudad</label>
                                <p>@Model.City</p>
                            </div>
                            <div class="form-group col-md-12 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Colonia</label>
                                <p>@Model.Colony</p>
                            </div>
                        </div>
                        <div class="form-row" style="-ms-flex-wrap:wrap; display:flex; margin-left:-5px; margin-right:-5px">
                            <div class="form-group col-md-8 col-lg-6" style="padding-left: 15px;padding-right: 15px;min-height:1px; position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Calles</label>
                                <p>@Model.Address</p>
                            </div>
                            <div class="form-group col-md-4 col-lg-3" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Código postal</label>
                                <p>@Model.PostalCode</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-3" style="padding-left: 15px;padding-right: 15px;min-height:1px; position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Teléfono de consultorio</label>
                                <p>@Model.OfficePhone</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-12" style="padding-left: 15px;padding-right: 15px;min-height:1px; position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Domicilio del consultorio</label>
                                <p>@Model.OfficeAddress</p>
                            </div>
                        </div>
                        <h5 class="mt-3" style="margin-bottom:1.5rem; margin-top:1rem; color:inherit; font-family:inherit; font-weight:500; line-height:1.2; font-size:1.2rem">Contacto</h5>
                        <div class="form-row" style="-ms-flex-wrap:wrap; display:flex;margin-left:-5px; margin-right:-5px">
                            <div class="form-group col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Teléfono</label>
                                <p>@Model.Phone</p>
                            </div>
                            <div class="form-group col-md-6 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px;position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Celular</label>
                                <p>@Model.CellPhone</p>
                            </div>
                            <div class="form-group col-md-12 col-lg-4" style="padding-left: 15px;padding-right: 15px;min-height:1px; position:relative; width:100%; margin-bottom:1rem" width="100%">
                                <label style="display:inline-block; margin-bottom:0.5rem">Correo(s) electrónico(s)</label>
                                <p>@Model.Email</p>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>
</div>